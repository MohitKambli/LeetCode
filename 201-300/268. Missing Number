class Solution {
    public int missingNumber(int[] nums) {
        int sum = 0, n = nums.length;
        for(int num : nums)
            sum += num;
        int actualSum = (n * (n + 1)) / 2;
        return actualSum - sum;
    }
}

/*class Solution {
    public int missingNumber(int[] nums) {
        int n = nums.length, ans = n;
        for(int i=0;i<n;i++)
            ans = ans ^ i ^ nums[i];
        return ans;
    }
}*/


class Solution(object):
    def missingNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        n = len(nums)
        total_sum = (n * (n + 1)) / 2
        actual_sum = sum(nums)
        return total_sum - actual_sum
