class Solution {
    public boolean isAnagram(String s, String t) {
        int m = s.length(), n = t.length();
        if(m != n)
            return false;
        int[] arr = new int[26];
        for(char ch : s.toCharArray())
            arr[ch - 'a']++;
        for(char ch : t.toCharArray())
            arr[ch - 'a']--;
        for(int i=0;i<26;i++)
            if(arr[i] != 0)
                return false;
        return true;
    }
}




class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        arr = [0 for i in range(26)]
        for ch in s:
            arr[ord(ch) - ord('a')] += 1
        for ch in t:
            arr[ord(ch) - ord('a')] -= 1
        for i in range(26):
            if arr[i] != 0:
                return False
        return True
        
