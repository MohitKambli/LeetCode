class Solution {
    public void moveZeroes(int[] nums) {
        int n = nums.length, j = 0;
        for(int i=0;i<n;i++)
            if(nums[i] != 0)
                nums[j++] = nums[i];
        while(j < n)
            nums[j++] = 0;
    }
}



class Solution(object):
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: None Do not return anything, modify nums in-place instead.
        """
        n, index = len(nums), 0
        for i in range(n):
            if nums[i] != 0:
                nums[index] = nums[i]
                index += 1
        for i in range(index, n):
            nums[i] = 0
