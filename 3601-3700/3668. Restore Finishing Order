class Solution {
    public int[] recoverOrder(int[] order, int[] friends) {
        Set<Integer> set = new HashSet<>();
        for(int friend : friends)
            set.add(friend);
        List<Integer> ans = new ArrayList<>();
        for(int o : order)
            if(set.contains(o))
                ans.add(o);
        int len = ans.size();
        int[] res = new int[len];
        for(int i=0;i<len;i++)
            res[i] = ans.get(i);
        return res;
    }
}
