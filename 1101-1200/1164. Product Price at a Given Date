# Write your MySQL query statement below
select
    p.product_id,
    p.new_price as price
from products p
where (p.product_id, p.change_date) in (
    select
        product_id,
        max(change_date)
    from products
    where change_date <= '2019-08-16'
    group by product_id
)
union
select
    distinct p.product_id,
    10 as price
from products p
where p.product_id not in (
    select
        product_id
    from products
    where change_date <= '2019-08-16'
);
