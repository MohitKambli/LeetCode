class Solution {
    public long countSubarrays(int[] nums, long k) {
        int n = nums.length, j = 0;
        long sum = 0, res = 0;
        for(int i=0;i<n;i++) {
            sum += nums[i];
            while(sum * (i - j + 1) >= k)
                sum -= nums[j++];
            res += i - j + 1;
        }
        return res;
    }
}
