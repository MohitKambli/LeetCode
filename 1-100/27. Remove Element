class Solution {
    public int removeElement(int[] nums, int val) {
        int n = nums.length, j = 0;
        for(int i=0;i<n;i++)
            if(nums[i] != val)
                nums[j++] = nums[i];
        return j;
    }
}



class Solution(object):
    def removeElement(self, nums, val):
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """
        n, k = len(nums), 0
        for i in range(n):
            if nums[i] != val:
                nums[k] = nums[i]
                k += 1
        return k
