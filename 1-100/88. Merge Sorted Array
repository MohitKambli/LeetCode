class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int N = nums1.length, i = 0, j = 0, k = 0;
        int[] arr = new int[N];
        while(i < m && j < n) {
            if(nums1[i] < nums2[j])
                arr[k++] = nums1[i++];
            else
                arr[k++] = nums2[j++];
        }
        while(i < m)
            arr[k++] = nums1[i++];
        while(j < n)
            arr[k++] = nums2[j++];
        int p = 0;
        while(p < N) {
            nums1[p] = arr[p];
            p++;
        }
    }
}


class Solution(object):
    def merge(self, nums1, m, nums2, n):
        """
        :type nums1: List[int]
        :type m: int
        :type nums2: List[int]
        :type n: int
        :rtype: None Do not return anything, modify nums1 in-place instead.
        """
        i, j, k = 0, 0, 0
        arr = [0 for q in range(m + n)]
        while i < m and j < n:
            if nums1[i] < nums2[j]:
                arr[k] = nums1[i]
                k += 1
                i += 1
            else:
                arr[k] = nums2[j]
                k += 1
                j += 1
        while i < m:
            arr[k] = nums1[i]
            k += 1
            i += 1
        while j < n:
            arr[k] = nums2[j]
            k += 1
            j += 1
        p = 0
        for x in arr:
            nums1[p] = x
            p += 1
